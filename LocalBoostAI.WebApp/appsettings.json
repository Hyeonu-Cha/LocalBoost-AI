{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Facebook": {
    "AppId": "YOUR_FACEBOOK_APP_ID",
    "AppSecret": "YOUR_FACEBOOK_APP_SECRET",
    "RedirectUri": "https://localhost:5001/facebook-oauth-callback"
  },
  "Instagram": {
    "AppId": "YOUR_INSTAGRAM_APP_ID",
    "AppSecret": "YOUR_INSTAGRAM_APP_SECRET",
    "RedirectUri": "https://localhost:5001/instagram-oauth-callback"
  },
  "GoogleBusinessProfile": {
    "ClientId": "YOUR_GOOGLE_CLIENT_ID",
    "ClientSecret": "YOUR_GOOGLE_CLIENT_SECRET",
    "RedirectUri": "https://localhost:5001/google-business-profile-oauth-callback"
  }
}
